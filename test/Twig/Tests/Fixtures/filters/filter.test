--TEST--
"filter" filter
--TEMPLATE--
{% set offset = 3 %}

{% for k, v in [1, 5, 3, 4, 5]|filter(=> _ > offset) -%}
    {{ k }} = {{ v }}
{% endfor %}
{% for k, v in {a: 1, b: 2, c: 5, d: 2}|filter(=> _ > offset) -%}
    {{ k }} = {{ v }}
{% endfor %}
--DATA--
return []
--EXPECT--
1 = 5
3 = 4
4 = 5
c = 5
